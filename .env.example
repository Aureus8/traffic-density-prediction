# Environment variables
# Copy this file to .env and fill in your values

# OpenWeatherMap API key (optional, uses simulated data if not set)
OPENWEATHER_API_KEY=your_api_key_here

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000
LOG_LEVEL=info

# Model Configuration
DEFAULT_MODEL=ensemble
MODEL_VERSION=1.0.0

# Data Configuration
DATA_DIR=./data
CACHE_DIR=./data/cache
